: 3dup ( a b c -- a b c a b c)
	dup 2over rot ;

: 3drop
	2drop drop ;

: within? ( n lo hi -- f)
	rot swap over > -rot <= and ;

: endpoint? ( n lo hi -- f)
	rot swap over = -rot = or ;

: within-strict? ( n lo hi -- f)
	swap 1+ swap within? ;

: trap ( n x y -- n {playing} | {won})
	3dup endpoint? if ." you got it!" cr 3drop exit then

	3dup within-strict? if ." between" cr else ." not between" cr
	then 2drop ;
