def groups_per_user(group_dictionary):
  user_groups = {}

  for group, users in group_dictionary.items():
    for user in users:
      listing = user_groups.get(user, [])
      user_groups[user] = listing.append(group)

  return(user_groups)

print(groups_per_user({"local": ["admin", "userA"],\
"public":  ["admin", "userB"], "administrator": ["admin"] }))
