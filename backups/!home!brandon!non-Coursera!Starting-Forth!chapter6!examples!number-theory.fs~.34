\ proof-of-concept template for a loop construct avoiding do...loop
: to-5 ( --)
	1 >r begin
		r> \ code here
		dup 5 < while
	1+ >r repeat rdrop ;

\ doesn't optimize to only check r@ up to flsq(n)
: divisor ( n -- divisor)
	2 >r begin
		dup r@ mod while
	r> 1+ >r repeat
	r> nip ;

: square ( n -- n * n)
	dup * ;

: sqrt ( n -- floor{sqrt{n}}) \ n > 0
	>r 1 begin
		1+ dup square r@
	> until
	1- rdrop ;

