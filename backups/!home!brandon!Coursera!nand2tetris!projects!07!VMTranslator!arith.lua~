local M = {}

-- A set of proto-commands from which the real commands are derived
-- with more generality.
local proto = {
	["nnn"] = [[
@SP
A=M-1
M=%sM]],

	["bin"] = [[
@SP
M=M-1
A=M
D=M
M=M-1
M=D%sM]],

	["cmp_0"] = [[
@SP
A=M-1
D=M
M=-1
@DONE
D;J%s
@SP
A=M-1
M=0
(DONE)]]
}

-- The real arithmetic-logic command-set
local commands = {}

commands["neg"] = string.format(
