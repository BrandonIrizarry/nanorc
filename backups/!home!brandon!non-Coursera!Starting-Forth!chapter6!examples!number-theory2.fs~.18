: square ( n -- n * n)
	dup * ;

: sqrt ( n -- floor{sqrt{n}})	\ valid for n > 0 only
	>r 1 begin
		1+ dup square r@
	> until
	1- rdrop ;

: evenly ( a b -- f) \ a is evenly divided by b
	mod 0= ;

\ : divisor ( n -- n)		\ compute smallest divisor of n
\ 	dup >r sqrt 1+ 2 do
\ 		i j divides if i leave then
\ 	loop r> min ;

: divisor ( n -- n)
	dup sqrt 1+ 2 do
		dup i evenly if drop i leave then
	loop ;

5 divisor . cr .s
