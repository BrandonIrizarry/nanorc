: 3dup ( a b c -- a b c a b c)
	dup 2over rot ;

: 3drop
	2drop drop ;

: within? ( n lo hi -- f)
	rot swap over > -rot <= and ;

: within-strict? ( n lo hi -- f)
 	swap 1+ swap within? ;

: all-equal? ( a b c -- f)
(	over  = -rot = and ;

: trap ( n x y -- n {playing} | {won})
	3dup within-strict? if ." between" cr 2drop exit then )

  	3dup all-equal? if
		." you got it!" cr 3drop
	else
		." not between" cr 2drop then ;

